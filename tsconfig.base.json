{
    "compilerOptions": {
      "target": "ES2019",
      "module": "ESNext",
      "moduleResolution": "Bundler",
  
      "declaration": true,
      "declarationMap": true,
      "sourceMap": true,
  
      "strict": true,
      "skipLibCheck": true,
  
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
  
      "outDir": "dist"
    }
  }
  