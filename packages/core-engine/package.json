{
  "name": "@threejs-shared/core-engine",
  "version": "0.1.15",
  "description": "Three.js engine core without binding to specific three version",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./adapter-hdr": {
      "types": "./dist/adapter-hdr.d.ts",
      "import": "./dist/adapter-hdr.js",
      "default": "./dist/adapter-hdr.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json"
  },
  "peerDependencies": {
    "three": ">=0.157.0"
  },
  "dependencies": {
    "earcut": "^2.2.4",
    "@threejs-shared/cache-db": "workspace:*"
  }
}
