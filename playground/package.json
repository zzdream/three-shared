{
  "name": "playground",
  "private": true,
  "version": "0.0.20",
  "lint-staged": {
    "src/**/*.{js,jsx,vue,ts,tsx}": [
      "npm run eslint"
    ]
  },
  "scripts": {
    "dev": "vue-tsc && vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "eslint": "eslint --max-warnings=0",
    "pre-check": "vue-tsc && npx lint-staged",
    "commit": "cz"
  },
  "dependencies": {
    "@jridgewell/sourcemap-codec": "^1.4.14",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vueuse/core": "^9.13.0",
    "autoprefixer": "^10.4.14",
    "axios": "^1.3.4",
    "eslint": "^8.37.0",
    "eslint-plugin-vue": "^9.10.0",
    "express": "^4.18.2",
    "less": "^4.1.3",
    "three": "^0.182.0",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.15.2",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.0",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.2.47",
    "vue-eslint-parser": "^9.1.1",
    "vue-request": "^2.0.0-rc.4",
    "vue-router": "^4.4.5",
    "vue-tsc": "^1.2.0",
    "@threejs-shared/wasm": "workspace:*",
    "@threejs-shared/xodr": "workspace:*",
    "@threejs-shared/core-engine": "workspace:*",
    "@threejs-shared/three-adapter-182": "workspace:*",
    "@threejs-shared/protobuf": "workspace:*"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "html-minifier-terser": "^7.2.0",
    "husky": "^8.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "terser": "^5.31.0",
    "vite-plugin-css-injected-by-js": "^3.5.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
